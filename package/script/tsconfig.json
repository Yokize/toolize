{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "diagnostics": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "inlineSources": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noErrorTruncation": true,
    "target": "es5",
    "sourceMap": true
  }
}
